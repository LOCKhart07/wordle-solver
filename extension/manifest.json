{
    "manifest_version": 2,
    "name": "Wordle Solver",
    "version": "1.5",
    "description": "Solves the Wordle game by analyzing the current state and providing suggestions.",
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://www.nytimes.com/games/wordle/index.html"
            ],
            "js": [
                "solver.js"
            ]
        }
    ],
    "permissions": [
        "https://raw.githubusercontent.com/"
    ]
}